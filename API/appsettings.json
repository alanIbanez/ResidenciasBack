{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=Residencias;Username=postgres;Password=your_password;Pooling=true;"
  },
  "JwtSettings": {
    "SecretKey": "YourSuperSecretKeyForJWTTokenGenerationWithAtLeast32Characters",
    "Issuer": "BackendNotificaciones",
    "Audience": "NotificationAppUsers",
    "ExpiryInMinutes": 60,
    "RefreshTokenExpiryInDays": 7
  },
  "Expo": {
    "ApiBaseUrl": "https://exp.host/--/api/v2/push/",
    "AccessToken": "your_expo_access_token_optional"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:19006",
      "exp://your-expo-url"
    ]
  }
}
